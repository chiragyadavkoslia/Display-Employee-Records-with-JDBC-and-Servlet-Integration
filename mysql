CREATE DATABASE CompanyDB;

USE CompanyDB;

CREATE TABLE Employee (
    EmpID INT PRIMARY KEY,
    Name VARCHAR(50),
    Salary DECIMAL(10,2)
);

-- Sample data
INSERT INTO Employee VALUES (1, 'Alice', 50000.00);
INSERT INTO Employee VALUES (2, 'Bob', 60000.00);
INSERT INTO Employee VALUES (3, 'Charlie', 55000.00);
